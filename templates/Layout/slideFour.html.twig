<!-- Four -->
<div class="slide four">
    <section class="center-form">
        <h2 id="title-form">Contactez-moi</h2>
        <div class="form-column">
            <div>
            {{ form_start(contactForm) }}
            <div class="form-group">
                <label for="name">Nom · 姓名</label>
                {{ form_widget(contactForm.name) }}
            </div>
            <div class="form-group">
                <label for="email">Email · 电子邮件</label>
                {{ form_widget(contactForm.email) }}
            </div>
            <div class="form-group">
                <label for="phoneNumber">Numéro · 数字</label>
                {{ form_widget(contactForm.phoneNumber) }}
            </div>
            <div class="form-group">
                <label for="message">Message · 信息</label>
                {{ form_widget(contactForm.message) }}
            </div>
            </div>
            <div class="btn-contact">
                <button type="submit">Send</button>
            </div>
            {{ form_end(contactForm) }}
        </div>
    </section>
</div>
